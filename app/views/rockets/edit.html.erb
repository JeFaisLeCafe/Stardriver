<div class="container">
  <div class="col-xs-12">
    <h1>Modify a big fu**ing rocket</h1>
    <%= simple_form_for [@rocket] do |f| %>
      <%= f.input :capacity %>
      <%= f.input :model %>
      <%= f.input :photo %>
      <%= f.input :photo_cache, as: :hidden %>
      <%= f.submit %>
    <% end %>
  </div>
</div>
